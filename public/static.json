{
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "dest": "/favicon.ico"
    },
    {
      "src": "/logo192.png",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "dest": "/logo192.png"
    },
    {
      "src": "/logo512.png",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "dest": "/logo512.png"
    },
    {
      "src": "/manifest.json",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
